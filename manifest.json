{
    "manifest_version": 2,
    "name": "upgrad speed",
    "description": "Increase the speed of video upto 4x",
    "version": "1.0",
    "browser_action": {
    "default_popup": "index.html"
    },
    "permissions": [],
    
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["js/jquery.min.js" , "content.js"]
        }
    ],
    "web_accessible_resources": [
      "css/*",
      "js/*"
 
    ]
    
}